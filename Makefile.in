@am__fastdepCC_TRUE@	$(AM_V_at)$(am__mv) $(DEPDIR)/mutt_md5-md5.Tpo $(DEPDIR)/mutt_md5-md5.Po
@AMDEP_TRUE@@am__fastdepCC_FALSE@	$(AM_V_CC)source='md5.c' object='mutt_md5-md5.obj' libtool=no @AMDEPBACKSLASH@
